{
    "name": "mpsoft/mpstock",
    "description": "Manage stock quantities",
    "authors": [
        {
            "name": "Massimiliano Palermo",
            "email": "maxx.palermo@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "prestashop/prestashop-webservice-lib": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "MpSoft\\MpStock\\": "src/",
            "MpSoft\\MpStock\\WebServices\\": "vendor/prestashop/prestashop-webservice-lib"
        },
        "classmap": [],
        "exclude-from-classmap": [],
        "files": [
            "vendor/prestashop/prestashop-webservice-lib/PSWebServiceLibrary.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false
    },
    "type": "prestashop-module"
}
